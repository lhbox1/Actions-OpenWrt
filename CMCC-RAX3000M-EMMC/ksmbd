
config globals
	option workgroup 'WORKGROUP'
	option description 'Ksmbd on OpenWrt'

config share
	option name 'RAX30000M'
	option path '/mnt/mmcblk0p7'
	option read_only 'no'
	option force_root '1'
	option guest_ok 'yes'
	option create_mask '0666'
	option dir_mask '0777'

config share
	option name '移动固态'
	option path '/mnt/sda1'
	option read_only 'no'
	option guest_ok 'yes'
	option create_mask '0666'
	option dir_mask '0777'

